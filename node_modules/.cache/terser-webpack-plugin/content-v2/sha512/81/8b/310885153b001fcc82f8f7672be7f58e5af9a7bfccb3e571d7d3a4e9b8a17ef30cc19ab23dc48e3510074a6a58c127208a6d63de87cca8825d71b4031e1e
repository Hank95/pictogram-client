Click(!isClick);\n  }\n\n  return (\n    <div className=\"Appz\">\n      <p className=\"Counter\">{count}</p>\n      <Heart className=\"Heart\" isClick={isClick} onClick={handleClick} />\n    </div>\n  );\n}\n\nexport default App;\n","import App from \"./Heart.js\";\n\nfunction LikeButton() {\n  return (\n    <>\n      <App />\n    </>\n  );\n}\n\nexport default LikeButton;\n","import React from \"react\";\nimport { Card, Image } from \"semantic-ui-react\";\nimport LikeButton from \"./LikeButton\";\n\nconst PictureCard = (post) => {\n  return (\n    <Card className=\"Card\">\n      <Image src={post.post.image} wrapped ui={false} />\n      <Card.Content className=\"CardContent\">\n        <Card.Header>{post.post.title}</Card.Header>\n        <Card.Meta>{post.post.author}</Card.Meta>\n        <Card.Meta>{post.post.datePosted}</Card.Meta>\n        <LikeButton />\n      </Card.Content>\n    </Card>\n  );\n};\n\nexport default PictureCard;\n","import PictureCard from \"./PictureCard\";\n\nconst Home = ({ feed }) => {\n  return (\n    <div className=\"feedList\" id=\"feedList\">\n      {feed.map((post) => {\n        return <PictureCard post={post} />;\n      })}\n    </div>\n  );\n};\n\nexport default Home;\n","const StyleBar = ({ handleData, formData }) => {\n  return (\n    <aside className=\"sidebar\">\n      <ul>\n        <li>\n          <label htmlFor=\"color\">Color:</label>\n          <input type=\"color\" id=\"color\" name=\"color\" onChange={handleData} />\n        </li>\n        <li>\n          <label htmlFor=\"weight\">Stroke:</label>\n          <input\n            type=\"number\"\n            id=\"weight\"\n            name=\"stroke\"\n            min=\"2\"\n            max=\"200\"\n            value={formData.stroke}\n            onChange={handleData}\n          />\n        </li>\n        <li>\n          <button id=\"clear\">\n            <i className=\"fa fa-trash\"></i>\n          </button>\n        </li>\n      </ul>\n    </aside>\n  );\n};\n\nexport defau